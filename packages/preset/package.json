{
  "name": "@gooey-ui/tailwind-preset",
  "version": "0.1.0",
  "description": "Tailwind CSS preset for Gooey UI components",
  "author": "Gooey UI Contributors",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "tailwindcss",
    "preset",
    "gooey-ui"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "tailwindcss": "^4.0.0-alpha.6",
    "typescript": "^5.4.2",
    "tsup": "^8.0.2"
  }
}
